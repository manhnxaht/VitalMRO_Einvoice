<odoo>
    <data noupdate="1">
        <!-- Cron Job for Checking E-Invoices -->
        <record id="ir_cron_get_submission_invoice" model="ir.cron">
            <field name="name">Submission E-Invoices</field>
            <field name="model_id" ref="model_account_move"/>
            <field name="state">code</field>
            <field name="code">model.cron_get_einvoice_submission_detail()</field>
            <field name="interval_number">5</field>
            <field name="interval_type">minutes</field>
            <field name="numbercall">-1</field>
        </record>
    </data>
</odoo>
